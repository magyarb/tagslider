 <h2 class="page-header">Dashboard</h2>
<p>Welcome to your dashboard {{username}}</p>
 <form method="get" action="/users/auth/instagram">
     <button color="blue" class="btn btn-default">Connect Instagram</button>
 </form>
 {{#if instaname}}
 <p>Instagram account {{instaname}} is connected.</p>
 {{/if}}
 <form method="get" action="/insta">
     <button color="blue" class="btn btn-default">Test Instagram</button>
 </form>
 <form method="post" action="/insta/tag">
     <div class="form-group">
         <label>hashtag</label>
         <input type="text" class="form-control" name="hashtag" placeholder="hashtag">
     </div>
     <button type="submit" class="btn btn-default">Submit</button>
 </form>